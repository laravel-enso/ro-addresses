{
    "timeout": 500,
    "sheets": [
        {
            "importerClass": "LaravelEnso\\RoAddresses\\App\\Imports\\Importers\\LocalityUpdateImporter",
            "validatorClass": "LaravelEnso\\RoAddresses\\App\\Imports\\Validators\\LocalityUpdateValidator",
            "name": "localitati",
            "chunkSize": 1000,
            "columns": [
                {
                    "name": "county",
                    "validations": "string|required|exists:counties,name"
                },
                {
                    "name": "township",
                    "validations": "string|nullable|exists:localities,township"
                },
                {
                    "name": "locality",
                    "validations": "string|required"
                },
                {
                    "name": "siruta",
                    "validations": "nullable"
                },
                {
                    "name": "is_active",
                    "validations": "required|in:0,1"
                },
                {
                    "name": "lat",
                    "validations": "numeric|nullable"
                },
                {
                    "name": "long",
                    "validations": "numeric|nullable"
                }
            ]
        }
    ]
}